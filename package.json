{
  "name": "@pixi/animate",
  "description": "PIXI plugin for the PixiAnimate Extension",
  "version": "2.0.0",
  "main": "lib/animate.js",
  "module": "lib/animate.es.js",
  "bundle": "dist/animate.js",
  "typings": "lib/animate.d.ts",
  "contributors": [
    "Matt Karl <matt@mattkarl.com>",
    "Eric Ente (https://github.com/ericente)",
    "Andrew Start (https://github.com/andrewstart)"
  ],
  "devDependencies": {
    "@pixi/app": "^6.0.1",
    "@pixi/core": "^6.0.1",
    "@pixi/display": "^6.0.1",
    "@pixi/eslint-config": "^1.0.1",
    "@pixi/filter-color-matrix": "^6.0.1",
    "@pixi/graphics": "^6.0.1",
    "@pixi/loaders": "^6.0.1",
    "@pixi/prepare": "^6.0.1",
    "@pixi/settings": "^6.0.1",
    "@pixi/sprite": "^6.0.1",
    "@pixi/spritesheet": "^6.0.1",
    "@pixi/text": "^6.0.1",
    "@pixi/ticker": "^6.0.1",
    "@pixi/utils": "^6.0.1",
    "@rollup/plugin-replace": "^2.3.1",
    "chai": "^4.2.0",
    "electron": "^13.6.6",
    "eslint": "^6.0.0",
    "floss": "^5.0.0",
    "gh-pages": "^2.0.0",
    "js-md5": "^0.4.1",
    "nyc": "^15.0.0",
    "pixi.js": "^6.0.1",
    "pixi.js-legacy": "^6.0.1",
    "rimraf": "^2.5.4",
    "rollup": "^1.32.0",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-jscc": "^1.0.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-ts": "^1.3.7",
    "semver": "^5.3.0",
    "terser": "^3.16.1",
    "tslib": "^1.9.3",
    "typedoc": "^0.20.28",
    "typescript": "^3.9.0"
  },
  "peerDependencies": {
    "@pixi/app": "^6.0.1",
    "@pixi/core": "^6.0.1",
    "@pixi/display": "^6.0.1",
    "@pixi/filter-color-matrix": "^6.0.1",
    "@pixi/graphics": "^6.0.1",
    "@pixi/loaders": "^6.0.1",
    "@pixi/prepare": "^6.0.1",
    "@pixi/settings": "^6.0.1",
    "@pixi/sprite": "^6.0.1",
    "@pixi/spritesheet": "^6.0.1",
    "@pixi/text": "^6.0.1",
    "@pixi/ticker": "^6.0.1",
    "@pixi/utils": "^6.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pixijs/animate"
  },
  "bugs": {
    "url": "https://github.com/pixijs/animate/issues"
  },
  "homepage": "https://github.com/pixijs/animate",
  "scripts": {
    "clean": "rimraf docs/docs docs/dist .publish",
    "lint": "eslint --ext .ts src",
    "prebuild": "npm run lint",
    "build": "tsc && rollup -c && npm run build:min",
    "build:min": "npm run build:min:bundle && npm run build:min:legacy",
    "build:min:bundle": "terser dist/animate.js -c -m -o dist/animate.min.js --source-map \"content='dist/animate.js.map',includeSources=true,filename='dist/animate.min.js.map',url='animate.min.js.map'\" --comments \"/animate.js - /\"",
    "build:min:legacy": "terser dist/animate-legacy.js -c -m -o dist/animate-legacy.min.js --source-map \"content='dist/animate-legacy.js.map',includeSources=true,filename='dist/animate-legacy.min.js.map',url='animate-legacy.min.js.map'\" --comments \"/animate-legacy.js - /\"",
    "test": "npm run test:floss && npm run test:tsc",
    "test:floss": "floss --path tests/index.js",
    "test:tsc": "tsc -p tests/ts-lib/tsconfig.json",
    "coverage": "nyc npm run test:floss && npm run test:tsc",
    "test:debug": "npm run test:floss -- --debug",
    "docs": "typedoc",
    "predocs-live": "npm run docs",
    "docs-live": "gh-pages -d docs",
    "postversion": "npm run build && npm test",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish",
    "postpublish": "git push && git push --tags && npm run docs-live"
  },
  "files": [
    "dist/",
    "lib/",
    "LICENSE",
    "package.json",
    "README.md",
    "bin/"
  ],
  "bin": {
    "animate-upgrade": "bin/assetConversion.js",
    "animate-type-assets": "bin/createDeclaration.js"
  },
  "keywords": [
    "webgl",
    "pixi",
    "pixi.js",
    "Flash",
    "Animate CC",
    "animation"
  ],
  "license": "MIT"
}
